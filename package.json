{
  "name": "eleventy",
  "version": "0.0.1",
  "date": "23.07.07",
  "license": "ISC",
  "author": "pasmurno by llcawc",
  "homepage": "https://github.com/llcawc",
  "description": "tailwind starter template by pasmurno on base 11ty and nunjucks",
  "scripts": {
    "arch": "rsync -aRv --delete --exclude=node_modules . /mnt/d/Coding/github/desk/",
    "clean": "rm -rf dist",
    "copy": "copyfiles -u1 'src/assets/fonts/**/*.*' dist",
    "htm": "npx @11ty/eleventy",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy",
    "css": "postcss src/assets/styles/*.css -d dist/assets/css --ext 'min.css'",
    "images": "imagemin src/assets/images --out-dir=dist/assets/images",
    "dev:js": "rollup -c -w --silent",
    "dev:img": "nodemon --ext jpg,png,svg -w './src' -x 'imagemin src/assets/images --out-dir=dist/assets/images'",
    "dev:watch": "nodemon --ext html,css,njk,md -w './src' -x 'run-s htm css' --ignore './dist'",
    "dev": "npm-run-all clean copy -p dev:* serve",
    "serve": "browser-sync start --config bs-config.js",
    "build": "run-s clean copy images build:* arch && echo 'CSS temporary file cleaned, production completed and archived'",
    "build:js": "rollup -c --environment BUILD:production",
    "build:min": "terser dist/assets/js/main.min.js --compress --output dist/assets/js/main.min.js",
    "build:css": "postcss src/assets/styles/*.css -d dist/assets/css --ext 'min.css' --env 'production'",
    "build:htm": "npx @11ty/eleventy",
    "build:htmlmin": "html-minifier-terser --collapse-whitespace --remove-comments --input-dir dist --file-ext html --output-dir dist"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "copyfiles": "^2.4.1",
    "html-minifier-terser": "^7.2.0",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "markdown-it": "^13.0.1",
    "nodemon": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.25",
    "postcss-cli": "^10.1.0",
    "postcss-csso": "^6.0.1",
    "postcss-import": "^15.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "rollup": "^3.26.2",
    "tailwindcss": "^3.3.2",
    "terser": "^5.18.2"
  }
}
